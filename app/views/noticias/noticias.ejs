<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="utf-8" />
    <title>Noticias</title>
  </head>
  <body>
    <h1>Noticias</h1>

    <table>
      <tr>
        <td>ID</td>
        <td>Titulo</td>
        <td>Noticia</td>
      </tr>

      <% noticias.forEach( noticia => { %>
      <tr>
        <td><%= noticia.id_noticia %></td>
        <td><%= noticia.titulo %></td>
        <td><%= noticia.noticia %></td>
      </tr>
     <% } ); %>
    </table>
  </body>
</html>
